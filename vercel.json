{
  "version": 2,
  "builds": [
    {
      "src": "app/backend/src/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "app/frontend/src/index.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
      {
          "src": "/api/.*",
          "dest": "app/backend/src/server.js"
      },
      {
          "src": "/.*",
          "dest": "app/frontend/src/index.js"
      }
  ]
}